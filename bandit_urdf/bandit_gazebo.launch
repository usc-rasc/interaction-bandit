  <node name="urdf_spawner" pkg="gazebo" type="spawn_model" respawn="false" output="screen" args="-urdf -model bandit -param robot_description"/>
    -->

  <node name="urdf_spawner" pkg="gazebo" type="spawn_model" respawn="false" output="screen" args="-file /home/astclair/workspaces/groovy/stacks/usc-interaction-software/bandit/bandit_urdf/urdf/bandit_defs.urdf -urdf -z 1 -model bandit"/>
  <!--
    -->

</launch>


